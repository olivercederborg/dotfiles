{
    "custom-ui-style.stylesheet": {
        ".quick-input-widget": {
            "left": "50% !important;",
            "margin": "0 !important;",
            "max-width": "800px !important;",
            "position": "fixed !important;",
            "top": "25% !important;",
            "transform": "translateX(-50%) !important; translateY(-50%) !important",
            "width": "60vw !important;",
            "@media (prefers-color-scheme: dark)": {
                "border": "1px solid #3c3836 !important;"
            }
        }
    },
    "vscode-neovim.afterInitConfig": [
        "set cmdheight=10",
        "set laststatus=2"
    ],
    "editor.bracketPairColorization.enabled": false,
    "editor.accessibilitySupport": "off",
    "editor.linkedEditing": true,
    "editor.inlineSuggest.enabled": true,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.codeLensFontFamily": "JetBrains Mono",
    "chat.editor.fontFamily": "JetBrains Mono",
    "editor.inlayHints.fontFamily": "JetBrains Mono",
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "editor.lineNumbers": "relative",
    "editor.fontSize": 14,
    "editor.lineHeight": 40,
    "terminal.integrated.fontSize": 15,
    "editor.fontLigatures": true,
    "editor.cursorBlinking": "solid",
    "workbench.tree.indent": 20,
    "editor.minimap.enabled": false,
    "editor.glyphMargin": false,
    "editor.cursorSurroundingLines": 7,
    "editor.cursorSmoothCaretAnimation": "off",
    "window.title": "${dirty}${activeEditorShort}",
    "workbench.editor.centeredLayoutAutoResize": false,
    "workbench.editor.showTabs": "single",
    "window.commandCenter": false,
    "window.titleBarStyle": "custom",
    "editor.minimap.renderCharacters": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.minimap.autohide": true,
    "editor.wordWrap": "bounded",
    "editor.wrappingStrategy": "advanced",
    "editor.colorDecorators": true,
    "git.openRepositoryInParentFolders": "always",
    "git.autofetch": true,
    "git.confirmNoVerifyCommit": false,
    "git.confirmSync": false,
    "git.ignoreMissingGitWarning": true,
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command",
        "push:command"
    ],
    "gitlens.hovers.currentLine.over": "line",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "tailwindCSS.experimental.classRegex": [
        [
            "cva\\(([^)]*)\\)",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "cn\\(([^)]*)\\)",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "cx\\(([^)]*)\\)",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ]
    ],
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.productIconTheme": "fluent-icons",
    "vscode-neovim.ctrlKeysForNormalMode": [
        "a",
        "b",
        "d",
        // "e",
        "f",
        "h",
        // "i",
        "j",
        // "k",
        "l",
        "m",
        "o",
        "r",
        "t",
        "u",
        "v",
        "w",
        "x",
        "y",
        "z",
        "/",
        "]"
    ],
    "vscode-neovim.ctrlKeysForInsertMode": [
        "a",
        "d",
        "h",
        "j",
        "m",
        "o",
        "r",
        "t",
        "u",
        "w"
    ],
    "githubPullRequests.pullBranch": "never",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "gitlens.views.remotes.branches.layout": "list",
    "security.workspace.trust.untrustedFiles": "open",
    "typescript.tsdk": "./node_modules/typescript/lib",
    "editor.wordWrapColumn": 120,
    "prettier.requireConfig": true,
    "workbench.sideBar.location": "right",
    "explorer.confirmDelete": false,
    "explorer.compactFolders": false,
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "explorer.confirmDragAndDrop": false,
    "git.ignoreRebaseWarning": true,
    "githubPullRequests.createOnPublishBranch": "never",
    "update.mode": "manual",
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.diffs.useCharacterLevelDiffs": true,
    "editor.formatOnSave": true,
    // Auto fix
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "never"
    },
    // Silent the stylistic rules in you IDE, but still auto fix them
    "eslint.rules.customizations": [
        {
            "rule": "style/*",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "format/*",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*-indent",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*-spacing",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*-spaces",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*-order",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*-dangle",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*-newline",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*quotes",
            "severity": "off",
            "fixable": true
        },
        {
            "rule": "*semi",
            "severity": "off",
            "fixable": true
        }
    ],
    // Enable eslint for all supported languages
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue",
        "html",
        "markdown",
        "json",
        "jsonc",
        "yaml",
        "toml",
        "xml",
        "gql",
        "graphql",
        "astro",
        "svelte",
        "css",
        "less",
        "scss",
        "pcss",
        "postcss"
    ],
    "eslint.useFlatConfig": true,
    "workbench.editor.customLabels.patterns": {
        "**/app/**/*slug*/page.jsx": "üêå ${dirname(1)}.${extname}",
        "**/app/**/*slug*/page.tsx": "üêå ${dirname(1)}.${extname}",
        "**/app/**/layout.jsx": "üñºÔ∏è ${dirname}.${extname}",
        "**/app/**/layout.tsx": "üñºÔ∏è ${dirname}.${extname}",
        "**/app/**/page.jsx": "üìÑ ${dirname}.${extname}",
        "**/app/**/page.tsx": "üìÑ ${dirname}.${extname}"
    },
    "cursor.cpp.enablePartialAccepts": true,
    "gitlens.hovers.avatars": false,
    "gitlens.hovers.enabled": false,
    "vscode-neovim.NVIM_APPNAME": "nvim",
    "workbench.activityBar.location": "hidden",
    "workbench.layoutControl.enabled": false,
    "editor.suggest.insertMode": "replace",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.inlayHints.fontSize": 15,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "vscode-neovim.logOutputToConsole": true,
    "cursor.composer.collapsePaneInputBoxPills": true,
    "security.promptForLocalFileProtocolHandling": false,
    "cursor.cpp.disabledLanguages": [],
    "workbench.statusBar.visible": false,
    "cursor.composer.cmdPFilePicker2": true,
    "editor.stickyScroll.maxLineCount": 3,
    "workbench.tree.stickyScrollMaxItemCount": 3,
    "workbench.editor.enablePreview": false,
    "window.autoDetectColorScheme": true,
    "workbench.preferredLightColorTheme": "GitHub Light",
    "workbench.preferredDarkColorTheme": "Gruvbox Dark Medium",
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "literals",
    "editor.inlayHints.enabled": "onUnlessPressed",
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "breadcrumbs.enabled": false,
}
